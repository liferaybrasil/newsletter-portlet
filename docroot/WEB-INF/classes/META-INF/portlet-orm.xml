<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.newsletter.model.impl.CampaignModelImpl">
		<attributes>
			<id name="campaignId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="title" />
			<basic name="content" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.newsletter.model.impl.ContactModelImpl">
		<attributes>
			<id name="contactId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="email" />
			<basic name="name" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.newsletter.model.impl.SendCampaignModelImpl">
		<attributes>
			<id name="sendCampaignId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="sendDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="emailSubject" />
			<basic name="senderName" />
			<basic name="senderEmail" />
			<basic name="campaignId" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.liferay.newsletter.model.impl.NewsletterLogModelImpl">
		<attributes>
			<id name="newsletterLogId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="sendCampaignId" />
			<basic name="contactId" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.newsletter.model.impl.CampaignImpl" name="Campaign">
		<table name="Newsletter_Campaign" />
		<attributes />
	</entity>
	<entity class="com.liferay.newsletter.model.impl.ContactImpl" name="Contact">
		<table name="Newsletter_Contact" />
		<attributes />
	</entity>
	<entity class="com.liferay.newsletter.model.impl.SendCampaignImpl" name="SendCampaign">
		<table name="Newsletter_SendCampaign" />
		<attributes />
	</entity>
	<entity class="com.liferay.newsletter.model.impl.NewsletterLogImpl" name="NewsletterLog">
		<table name="Newsletter_NewsletterLog" />
		<attributes />
	</entity>
</entity-mappings>